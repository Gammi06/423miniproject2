<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="site.metacoding.miniproject2.domain.codes.PositionsCodeDao">

	<select id="findAll"
		resultType="site.metacoding.miniproject2.dto.CodesRespDto.PositionsCodeRespDto">
		SELECT * FROM positions_code
	</select>
	
	<select id="findById"
		resultType="site.metacoding.miniproject2.dto.CodesRespDto.PositionsCodeRespDto">
		SELECT * FROM positions_code WHERE id = #{id}
	</select>
	
</mapper>