<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="site.metacoding.miniproject2.domain.codes.RegionsCodeDao">

	<select id="findAll"
		resultType="site.metacoding.miniproject2.web.dto.response.codes.RegionsCodeDto">
